{ l_output2=&quot;&quot; l_valid_shells=&quot;^($( awk -F/ &apos;$NF != &quot;nologin&quot; {print}&apos; /etc/shells | sed -rn &apos;/^//{s,/,\\/,g;p}&apos; | paste -s -d &apos;|&apos; - ))$&quot;
