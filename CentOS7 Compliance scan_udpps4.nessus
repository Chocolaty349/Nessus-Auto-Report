         } &gt;&gt; &quot;$l_gpfile&quot;
      fi
      # create dconf directory if it doesn&apos;t exists l_gpdir=&quot;/etc/dconf/db/$l_gpname.d&quot;
      if [ -d &quot;$l_gpdir&quot; ]; then
         echo &quot; - The dconf database directory \&quot;$l_gpdir\&quot; exists&quot;
      else
         echo &quot; - creating dconf database directory \&quot;$l_gpdir\&quot;&quot;
         mkdir &quot;$l_gpdir&quot;
